check process khulnasoft-analysisd
  matching "khulnasoft-analysisd"
  start program "/var/vcap/jobs/khulnasoft-manager-worker/bin/khulnasoft_server_ctl start"
  stop program "/var/vcap/jobs/khulnasoft-manager-worker/bin/khulnasoft_server_ctl stop"
  group vcap

